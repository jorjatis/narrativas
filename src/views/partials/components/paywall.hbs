                          handleCustomWidget(product);
                          handleCustomButtonText(product);
                        });

                        function handleCustomWidget(product) {
                          for (let paymentPlan of product.paymentPlans) {
                            createCustomPaymentPlanBox(product, paymentPlan);
                          }
                        }

                        function createCustomPaymentPlanBox(product, paymentPlan) {
                          const intervalPointer = setInterval(() => {
                            const productEls = document.querySelectorAll("#product-container-muro #product-" + product.name);
                            for (let productEl of productEls) {
                              if (productEl !== null) {
                                clearInterval(intervalPointer);
                                let PplanID = "#payment-plan-" + product.name + "-" + paymentPlan.name;
                                let pplanEl = productEl.querySelector(PplanID);
                                if (pplanEl === null) {
                                  continue;
                                }
                                if (product.paymentPlans.length == 1) {
                                  productEl.classList += " one-payment-plan";
                                  let productFeaturesEl = productEl.querySelector(".payment-plan-features");
                                  let productLinkEl = productEl.querySelector(".custom-feature");
                                  if (productFeaturesEl === null && paymentPlan.features && paymentPlan.features.length > 0) {
                                    addFeaturesPPlan(productEl, paymentPlan.features);
                                  } else if (productFeaturesEl === null && productLinkEl === null) {
                                    addCustomFeature(productEl);
                                  }
                                }
                                if (pplanEl) {
                                  pplanEl.textContent = "";
                                  //add checkbox
                                  createCheckbox(pplanEl);
                                  //add last hours claimer and recommended
                                  /*
                                  if (paymentPlan.id === product.autoSelectPlan) {
                                      addRecommendedBanner(pplanEl);
                                      addLastHoursClaimer(pplanEl);
                                      pplanEl.classList += " payment-plan-recommended";
                                  }
                                  */
                                  if (showRecommended.includes(paymentPlan.name) && pplanEl.parentElement.parentElement.parentElement.parentElement.querySelector(".recommended-banner") === null) {
                                    addRecommendedBanner(pplanEl, true);
                                    pplanEl.classList += " payment-plan-recommended";
                                  } else if (pplanEl.parentElement.parentElement.parentElement.parentElement.querySelector(".recommended-banner") === null) {
                                    addRecommendedBanner(pplanEl, false);
                                  }
                                  if (showClaimer.includes(paymentPlan.name)) {
                                    addLastHoursClaimer(pplanEl, true);
                                  } else {
                                    addLastHoursClaimer(pplanEl, false);
                                  }
                                  //add pplanName
                                  addPplanName(pplanEl, paymentPlan.displayName);
                                  //add price
                                  if (paymentPlan.trialPrice !== null) { // price with discount
                                    addStrikedPrice(pplanEl, paymentPlan.finalPrice, paymentPlan.currency.symbol, true);
                                    addFinalPrice(pplanEl, paymentPlan.trialPrice, paymentPlan.currency.symbol);
                                  } else { // price without discount
                                    addStrikedPrice(pplanEl, "", "", false);
                                    addFinalPrice(pplanEl, paymentPlan.finalPrice, paymentPlan.currency.symbol);
                                  }
                                  // add features
                                  if (product.paymentPlans.length > 1) {
                                    if (paymentPlan.id === product.autoSelectPlan && paymentPlan.features && paymentPlan.features.length > 0) {
                                      addFeaturesPPlan(pplanEl, paymentPlan.features);
                                    } else {
                                      addCustomFeature(pplanEl);
                                    }
                                  }
                                }
                                showProductsIfIsEveryProductReady(product, false);
                              }
                            }
                          }, 100);
                        }

                        function addRecommendedBanner(node, boolean) {
                          if (boolean) {
                            let recommendedEl = createCustomElement("span", "recommended-banner", "Recomendada");
                            node.parentElement.parentElement.parentElement.parentElement.prepend(recommendedEl)
                          } else {
                            let recommendedEl = createCustomElement("span", "recommended-banner visibility-hidden", "");
                            node.parentElement.parentElement.parentElement.parentElement.prepend(recommendedEl)
                          }
                        }

                        function createCheckbox(pplanEl) {
                          const checkbox = document.createElement("input");
                          checkbox.setAttribute("type", "checkbox");
                          checkbox.className = "plan-checkbox";
                          pplanEl.append(checkbox);
                        }

                        function addCustomFeature(node) {
                          let customFeature = createCustomElement("span", "custom-feature", "Renovación ");
                          let customLink = createCustomElement("a", "custom-feature-link", "consultar tarifas", "https://secure.abc.es/suscripcion/tarifas-vigentes");
                          customFeature.append(customLink);
                          node.append(customFeature);
                        }

                        function addFeaturesPPlan(node, pplanFeatures) {
                          let featureList = createCustomElement("ul", "payment-plan-features");
                          for (let feature of pplanFeatures) {
                            let featureEl = createCustomElement("li", "payment-plan-feature", feature);
                            featureList.append(featureEl);
                          }
                          node.append(featureList);
                        }

                        function addFinalPrice(pplanNode, price, currency) {
                          let priceContainer = createCustomElement("div", "payment-plan-final-price");
                          let priceArr = price.split(".");
                          let bigPrice = createCustomElement("span", "payment-plan-final-price-int", priceArr[0]);
                          let smallPriceText = (priceArr[1]) ? ("," + priceArr[1] + currency) : (",00" + currency);
                          let smallPrice = createCustomElement("span", "payment-plan-final-price-decimal", smallPriceText);
                          priceContainer.append(bigPrice, smallPrice);
                          pplanNode.append(priceContainer);
                        }

                        function addStrikedPrice(pplanNode, finalPrice, currency, boolean) {
                          let strikedPriceContainer = createCustomElement("div", "payment-plan-old-price");
                          let beforeText = createCustomElement("span", "payment-plan-old-price-text", "Antes ");
                          let price = finalPrice + currency;
                          let strikedPrice = createCustomElement("span", "payment-plan-old-price-striked", price);
                          if (!boolean) {
                            strikedPriceContainer.classList.add("visibility-hidden");
                          }
                          strikedPriceContainer.append(beforeText, strikedPrice);
                          pplanNode.append(strikedPriceContainer);
                        }

                        function addPplanName(pplanNode, pplanName) {
                          let pplanNameEl = createCustomElement("span", "payment-plan-name", pplanName);
                          pplanNode.append(pplanNameEl);
                        }

                        function addLastHoursClaimer(pplanNode, boolean) {
                          let lastHoursPromoEl = createCustomElement("span", "payment-plan-last-hours-offer");
                          let lastHoursEl;
                          let promoEl;
                          if (boolean) {
                            lastHoursEl = createCustomElement("span", "payment-plan-last-hours-offer-bold", claimerTextBold);
                            promoEl = createCustomElement("span", "payment-plan-last-hours-offer-light", claimerTextLight);
                          } else {
                            lastHoursPromoEl.classList.add("visibility-hidden");
                            lastHoursEl = createCustomElement("span", "payment-plan-last-hours-offer-bold", "");
                            promoEl = createCustomElement("span", "payment-plan-last-hours-offer-light", "");
                          }
                          lastHoursPromoEl.append(lastHoursEl, promoEl);
                          pplanNode.append(lastHoursPromoEl);
                        }

                        function createCustomElement(elementType, className, content, href) {
                          let customEl = document.createElement(elementType);
                          if (className) {
                            customEl.className = className;
                          }
                          if (content) {
                            customEl.textContent = content;
                          }
                          if (elementType === "a") {
                            customEl.href = href;
                            customEl.setAttribute("target", "_blank");
                          }
                          return customEl;
                        }

                        function handleCustomButtonText(product) {
                          const intervalPointer = setInterval(() => {
                            let cont = 0;
                            const productEls = document.querySelectorAll("#product-container-muro #product-" + product.name);
                            for (let productEl of productEls) {
                              cont++;
                              const button = productEl.querySelector("button.btn.btn-default.product-select-btn");
                              if (productEl && button) {
                                button.textContent = "Suscríbete";
                                clearInterval(intervalPointer);
                              }
                              if (cont <= 2000) {
                                clearInterval(intervalPointer);
                              }
                            }
                          }, 100);
                        }

                        function moveProducts() {
                          const container = document.querySelector("#product-container-muro");
                          const evElOne = document.querySelector("#ev_em_product_one_muro");
                          const evElTwo = document.querySelector("#ev_em_product_two_muro");
                          if (container && evElOne && evElTwo) {
                            container.textContent = "";
                            container.append(evElOne, evElTwo);
                          }
                        }
                        let totalNodes = document.querySelectorAll("ev-engagement[group-name='paywall-abc'] ev-em-product-selection");
                        let infoNodes = [];
                        totalNodes.forEach((element) => {
                          let obj = {
                            product: element.getAttribute("product-name"),
                            paymentPlan: element.getAttribute("payment-plans"),
                            hasPromoCode: element.hasAttribute("promo-code"),
                            readyToSend: false,
                          };
                          infoNodes.push(obj);
                        });

                        function showProductsIfIsEveryProductReady(product, promoAction) {
                          infoNodes.forEach((element) => {
                            if (element['product'] === product.name && (product.paymentPlans.some(plan => plan.name === element['paymentPlan']) || element['paymentPlan'] === null)) {
                              if (promoAction) {
                                // En el evento promoAction, establece readyToSend solo si promoCodeApplied es true
                                element.readyToSend = true;
                              } else {
                                // En el evento loaded, solo establece readyToSend si no requiere promoCode o si promoCodeApplied es true
                                if (!element.hasPromoCode || element.promoCodeApplied === true) {
                                  element.readyToSend = true;
                                }
                              }
                            }
                          });
                          if (infoNodes.every(item => item.readyToSend)) {
                            const element = document.querySelector("ev-engagement[group-name='paywall-abc'] #paywall-abc");
                            element.style.display = "block";
                            const footer = document.querySelector("ev-engagement[group-name='paywall-abc'] .product-footer-info");
                            footer.style.display = "inline-block";
                            let skeletonEl = document.querySelector("#skeleton-loader");
                            skeletonEl.style.display = "none";
                          }
                        }
                      }(window, document))
                    </script>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </ev-engagement>
